{
  "name": "react-deep-link",
  "version": "0.1.0",
  "description": "A React mixin for linking form fields to a deep structure of data within the component's state.",
  "main": "lib/mixin.js",
  "scripts": {
    "compile": "babel ./src -d ./lib",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TrigenSoftware/react-deep-link.git"
  },
  "keywords": [
    "react",
    "link",
    "state",
    "linkstate",
    "flux",
    "form"
  ],
  "author": "dangreen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TrigenSoftware/react-deep-link/issues"
  },
  "homepage": "https://github.com/TrigenSoftware/react-deep-link",
  "dependencies": {
    "lodash": "^3.6.0",
    "react-addons-update": "^0.14.3",
    "react": "^0.14.3"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-plugin-add-module-exports": "^0.1.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
